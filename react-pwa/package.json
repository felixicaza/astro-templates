{
  "name": "react-pwa",
  "version": "0.0.1",
  "description": "React PWA template with performance and SEO configurations for Astro.",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "prepare": "simple-git-hooks",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "3.6.3",
    "@playform/compress": "0.1.6",
    "@playform/inline": "0.1.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vite-pwa/astro": "0.4.3",
    "astro": "4.16.14",
    "astro-compressor": "0.4.1",
    "astro-iconify": "1.2.0",
    "astro-seo-schema": "4.0.3",
    "astro-sitemap": "1.0.0",
    "astro-tunnel": "0.1.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "schema-dts": "1.1.2",
    "typescript": "5.6.3",
    "vite-plugin-pwa": "0.21.0",
    "workbox-window": "7.3.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.52.3",
    "@eslint/js": "9.32.0",
    "@typescript-eslint/parser": "8.38.0",
    "eslint": "9.32.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.47.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-yml": "1.18.0",
    "globals": "16.3.0",
    "jiti": "2.5.1",
    "nano-staged": "0.8.0",
    "neostandard": "0.12.2",
    "simple-git-hooks": "2.13.0"
  }
}
