{
  "name": "astro-templates",
  "version": "1.0.0",
  "private": true,
  "description": "A collection of Astro templates (https://astro.build/)",
  "keywords": [
    "template",
    "starter-template",
    "astro",
    "astrobuild",
    "astro-template"
  ],
  "homepage": "https://github.com/felixicaza/astro-templates#readme",
  "bugs": {
    "url": "https://github.com/felixicaza/astro-templates/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixicaza/astro-templates.git"
  },
  "author": "Felix Icaza <contacto@felixicaza.com>",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "pnpm -r preview"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@typescript-eslint/parser": "8.50.0",
    "eslint": "9.39.2",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.85.0",
    "eslint-plugin-yml": "1.19.1",
    "globals": "16.5.0",
    "jiti": "2.6.1",
    "nano-staged": "0.9.0",
    "neostandard": "0.12.2",
    "simple-git-hooks": "2.13.1"
  }
}
