{
  "name": "astro-template",
  "version": "0.0.1",
  "description": "Template with performance and SEO configurations for Astro.",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "prepare": "simple-git-hooks",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@playform/compress": "0.2.1",
    "@playform/inline": "0.1.2",
    "@sardine/lightningcss-plugin-global-custom-queries": "1.0.1",
    "@unocss/reset": "66.6.0",
    "astro": "5.17.1",
    "astro-capo": "0.0.1",
    "astro-compressor": "1.2.0",
    "astro-iconify": "1.2.0",
    "astro-lqip": "1.7.2",
    "astro-seo-schema": "5.1.0",
    "astro-sitemap": "1.0.0",
    "lightningcss": "1.31.1",
    "schema-dts": "1.1.5",
    "sharp": "0.34.5",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.88.2",
    "eslint-plugin-yml": "3.0.0",
    "globals": "17.2.0",
    "jiti": "2.6.1",
    "nano-staged": "0.9.0",
    "neostandard": "0.12.2",
    "simple-git-hooks": "2.13.1"
  }
}
