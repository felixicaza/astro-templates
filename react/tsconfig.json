{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@data/*": ["src/data/*"],
      "@layouts/*": ["src/layouts/*"],
      "@scripts/*": ["src/scripts/*"],
      "@sections/*": ["src/pages/_*"],
      "@styles/*": ["src/styles/*"],
      "@types": ["src/types"],
      "@ui/*": ["src/components/ui/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
