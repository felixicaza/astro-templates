{
  "name": "pwa",
  "version": "0.0.1",
  "description": "PWA template with performance and SEO configurations for Astro.",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "prepare": "husky",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "lint-staged": {
    "./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix --ext .js,.mjs,.cjs ./"
    ],
    "./**/*.ts": [
      "eslint --cache --fix --ext .ts ./"
    ],
    "./src/**/*.astro": [
      "eslint --cache --fix --ext .astro ./src/"
    ]
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/tailwind": "5.1.2",
    "@playform/compress": "0.1.6",
    "@playform/inline": "0.1.0",
    "@vite-pwa/astro": "0.4.3",
    "astro": "4.16.14",
    "astro-compressor": "0.4.1",
    "astro-iconify": "1.2.0",
    "astro-seo-schema": "4.0.3",
    "astro-sitemap": "1.0.0",
    "astro-tunnel": "0.1.6",
    "schema-dts": "1.1.2",
    "tailwindcss": "3.4.15",
    "typescript": "5.6.3",
    "vite-plugin-pwa": "0.21.0",
    "workbox-window": "7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@typescript-eslint/parser": "8.38.0",
    "eslint": "9.32.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.47.0",
    "eslint-plugin-yml": "1.18.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "jiti": "2.5.1",
    "lint-staged": "15.2.10",
    "neostandard": "0.12.2"
  }
}
